{
  "exclude": [
    "node_modules",
    "**/node_modules",
    "**/.cache/*",
    "**/public",
    "**/cypress"
  ],
  "include": [
    "./src/",
    ".eslintrc.js",
    "**/gatsby/**/*",
    "**/src/**/*",
    "**/__mocks__/**/*",
    "**/test/**/*",
    "**/index.ts"
  ],
  "compilerOptions": {
    "jsx": "react",
    "lib": ["dom", "es2015", "es2017", "es2019"],
    "baseUrl": "./",
    "paths": { "@types/google.maps": ["node_modules/@types/google.maps"] },
    "typeRoots": ["types", "node_modules/@types", "src/@types"],
    "allowJs": true,
    "incremental": true,
    "target": "esnext",
    "module": "CommonJS",
    "checkJs": true,
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
